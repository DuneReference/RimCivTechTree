<?xml version="1.0" encoding="utf-8" ?>
<Patch>

    <!-- Moving Individual items to different techs -->

    <Operation Class="PatchOperationAdd">
        <xpath>
            Defs/TerrainDef[
                @Name = "SteelBridgeBase"
            ]
        </xpath>
        <value>
            <researchPrerequisites Inherit="False">
                <li>DuneRef_Industrialization</li>
            </researchPrerequisites>
        </value>
    </Operation>

    <Operation Class="PatchOperationAdd">
        <xpath>
            Defs/ThingDef[
                defName = "VFEArch_HedgeWall"
            ]
        </xpath>
        <value>
            <researchPrerequisites Inherit="False">
                <li>DuneRef_Irrigation</li>
            </researchPrerequisites>
        </value>
    </Operation>

    <Operation Class="PatchOperationAdd">
        <xpath>
            Defs/TerrainDef[
                @Name = "VFEArch_WoodenParquetBase"
            ]
        </xpath>
        <value>
            <researchPrerequisites Inherit="False">
                <li>DuneRef_Woodworking</li>
            </researchPrerequisites>
        </value>
    </Operation>

    <Operation Class="PatchOperationAdd">
        <xpath>
            Defs/TerrainDef[
                @Name = "CobblestoneBase"
            ]
        </xpath>
        <value>
            <researchPrerequisites Inherit="False">
                <li>DuneRef_Masonry</li>
            </researchPrerequisites>
        </value>
    </Operation>

    <Operation Class="PatchOperationReplace">
        <xpath>
            Defs/TerrainDef[
                @Name = "StoneBridgeBase"
            ]/researchPrerequisites
        </xpath>
        <value>
            <researchPrerequisites Inherit="False">
                <li>DuneRef_Engineering</li>
            </researchPrerequisites>
        </value>
    </Operation>

    <Operation Class="PatchOperationReplace">
        <xpath>
            Defs/TerrainDef[
                defName = "VFEArch_ConcreteFoundation"
            ]/researchPrerequisites
        </xpath>
        <value>
            <researchPrerequisites Inherit="False">
                <li>DuneRef_Industrialization</li>
            </researchPrerequisites>
        </value>
    </Operation>

    <Operation Class="PatchOperationAdd">
        <xpath>
            Defs/TerrainDef[
                   defName = "VFEArch_FineWoodenFloor"
            ]
        </xpath>
        <value>
            <researchPrerequisites Inherit="False">
                <li>DuneRef_Woodworking</li>
            </researchPrerequisites>
        </value>
    </Operation>

    <Operation Class="PatchOperationReplace">
        <xpath>
            Defs/ThingDef[
                   defName = "VFEArch_ArmoredDoor"
                or defName = "VFEArch_ArmoredWall"
            ]/researchPrerequisites
        </xpath>
        <value>
            <researchPrerequisites Inherit="False">
                <li>DuneRef_BrutalWarfare</li>
            </researchPrerequisites>
        </value>
    </Operation>

    <Operation Class="PatchOperationReplace">
        <xpath>
            Defs/ThingDef[
                   defName = "VFEArch_BrickFence"
            ]/researchPrerequisites
        </xpath>
        <value>
            <researchPrerequisites Inherit="False">
                <li>DuneRef_Masonry</li>
            </researchPrerequisites>
        </value>
    </Operation>

    <Operation Class="PatchOperationAdd">
        <xpath>
            Defs/ThingDef[
                   defName = "VFEArch_AnimalGate"
                or defName = "VFEArch_PicketFence"
                or defName = "VFEArch_LowFence"
            ]
        </xpath>
        <value>
            <researchPrerequisites Inherit="False">
                <li>DuneRef_AnimalHusbandry</li>
            </researchPrerequisites>
        </value>
    </Operation>

    <Operation Class="PatchOperationAdd">
        <xpath>
            Defs/ThingDef[
                   defName = "VFEArch_SingleDoor"
                or defName = "VFEArch_SingleDoor_Flipped"
                or defName = "VFEArch_LogWall"
                or defName = "VFEArch_FineWall"
            ]
        </xpath>
        <value>
            <researchPrerequisites Inherit="False">
                <li>DuneRef_Woodworking</li>
            </researchPrerequisites>
        </value>
    </Operation>

    <Operation Class="PatchOperationAdd">
        <xpath>
            Defs/TerrainDef[
                   defName = "VFEArch_WoodPlankFloorWalnut"
                or defName = "VFEArch_WoodPlankFloorBirch"
                or defName = "VFEArch_WoodPlankFloorMahogany"
                or defName = "VFEArch_WoodPlankFloorConcrete"
                or defName = "VFEArch_FineWoodenFloorWalnut"
                or defName = "VFEArch_FineWoodenFloorBirch"
                or defName = "VFEArch_FineWoodenFloorMahogany"
            ]
        </xpath>
        <value>
            <researchPrerequisites Inherit="False">
                <li>DuneRef_Woodworking</li>
            </researchPrerequisites>
        </value>
    </Operation>

    <Operation Class="PatchOperationAdd">
        <xpath>
            Defs/TerrainDef[
                   defName = "VFEArch_FineGoldTile"
                or defName = "VFEArch_FineSilverTile"
            ]
        </xpath>
        <value>
            <researchPrerequisites Inherit="False">
                <li>DuneRef_Currency</li>
                <li>DuneRef_Masonry</li>
            </researchPrerequisites>
        </value>
    </Operation>

    <Operation Class="PatchOperationAdd">
        <xpath>
            Defs/TerrainDef[
                   defName = "VFEArch_FineSteelTile"
            ]
        </xpath>
        <value>
            <researchPrerequisites Inherit="False">
                <li>DuneRef_Steelworking</li>
                <li>DuneRef_Masonry</li>
            </researchPrerequisites>
        </value>
    </Operation>

    <Operation Class="PatchOperationReplace">
        <xpath>
            Defs/TerrainDef[
                   @Name = "PolishedStoneFloorBase"
            ]/researchPrerequisites
        </xpath>
        <value>
            <researchPrerequisites Inherit="False">
                <li>DuneRef_Masonry</li>
            </researchPrerequisites>
        </value>
    </Operation>

</Patch>